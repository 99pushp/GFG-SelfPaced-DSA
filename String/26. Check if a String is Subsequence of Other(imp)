 bool isSubSequence(string A, string B)
    {
        //code here
        int i=0;
        int j=0;
        while(j<B.length()){
            if(A[i]==B[j]){
                i++;j++;
            }
            else{
                j++;
            }
        }
        
        if(i==A.length()){
            return 1;
        }
        return 0;
    }
